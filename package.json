{
  "name": "carousel-lit",
  "version": "0.2.2",
  "author": "Joel Voss <mail@joelvoss.com>",
  "license": "MIT",
  "homepage": "https://github.com/joelvoss/carousel-lit",
  "bugs": {
    "url": "https://github.com/joelvoss/carousel-lit/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joelvoss/carousel-lit.git"
  },
  "main": "dist/cjs/index.cjs.js",
  "module": "dist/esm/index.es.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "jvdx build rollup 'src/index.tsx' -f cjs,esm -c",
    "lint": "jvdx lint",
    "format": "jvdx format",
    "test": "jvdx test",
    "validate": "npm run format && npm run lint && npm run test",
    "clean": "jvdx clean dist example/dist example/node_modules example/yarn.lock",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "@babel/runtime": "7.9.6",
    "@emotion/core": "10.0.28",
    "memoize-one": "5.1.1",
    "raf-schd": "4.0.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "resize-observer-polyfill": "1.5.1",
    "smoothscroll-polyfill": "0.4.4"
  },
  "devDependencies": {
    "@types/jest": "25.2.1",
    "@types/raf-schd": "4.0.0",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.7",
    "@types/smoothscroll-polyfill": "0.3.1",
    "jvdx": "1.2.0"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "jvdx pre-commit"
    }
  }
}
